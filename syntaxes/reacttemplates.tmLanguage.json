{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "React Templates",
	"patterns": [
		{
			"include": "text.html.basic"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.reacttemplates",
				"match": "\\b(rt-if|rt-repeat|rt-scope|rt-props|rt-class|rt-import|rt-require|rt-template|rt-include|rt-pre)\\b"
			
			}]
		},
		"strings": {
			"name": "string.quoted.double.reacttemplates",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.reacttemplates",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.rt"
}
